{
  "name": "Slick Notes",
  "version": "1.0",
  "description": "Save content from web pages directly to your Google Doc just by highlighting it.",
  "manifest_version": 2,
  "key": "",
  "browser_action": {
    "default_title": "Slick Notes",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/images/slick_notes16.png",
    "32": "/images/slick_notes32.png",
    "48": "/images/slick_notes48.png",
    "96": "/images/slick_notes96.png",
    "128": "/images/slick_notes128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["identity"],
  "oauth2": {
    "client_id": "410105494772-5pi3tod0m8eiaudujqqqhkt4peb8k4sq.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/documents",
      "https://www.googleapis.com/auth/drive.metadata.readonly"
    ]
  }
}
